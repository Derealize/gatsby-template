{
  "name": "gatsby-template",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-template",
  "author": "zicjin",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write **/*.{js,jsx,ts,tsx} --config ./.prettierrc"
  },
  "derealize": {
    "runScript": "develop",
    "formatScript": "format",
    "baseUrl": "http://localhost:8000",
    "port": 8000,
    "pages": [
      "/",
      "/Welcome",
      "/Play",
      "/Sizing",
      "/Color",
      "/Typography",
      "/Shadows",
      "/build-anything/Simple",
      "/build-anything/Playful",
      "/build-anything/Elegant",
      "/build-anything/Brutalist",
      "/Performance",
      "/Responsive",
      "/States",
      "/component/Recipes",
      "/ApplyDirective",
      "/Dark",
      "/Grid",
      "/Transforms",
      "/Gradients",
      "/marketing/Hero",
      "/marketing/Feature",
      "/marketing/CTA",
      "/marketing/Headers",
      "/marketing/Banners",
      "/application-ui/Layouts",
      "/application-ui/Headings",
      "/application-ui/DescriptionLists",
      "/application-ui/Tables",
      "/application-ui/Navbars",
      "/application-ui/Pagination",
      "/application-ui/Modals",
      "/application-ui/SlideOvers",
      "/application-ui/Avatars",
      "/application-ui/Dropdowns",
      "/application-ui/form/FormLayout",
      "/application-ui/form/InputGroups",
      "/application-ui/form/SelectMenus",
      "/application-ui/form/SignIn"
    ],
    "assetsPath": "./public/assets",
    "assetsUrl": "/assets",
    "applyCssFile": "./src/styles/globals.css"
  },
  "dependencies": {
    "gatsby": "^3.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@derealize/babel-plugin-transform-react": "^1.0.7",
    "autoprefixer": "^10.2.6",
    "babel-preset-gatsby": "^1.6.0",
    "gatsby-plugin-postcss": "^4.6.0",
    "postcss": "^8.3.0",
    "tailwindcss": "^2.1.4"
  }
}
